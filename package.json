{
  "name": "todoo",
  "productName": "Todoo",
  "private": true,
  "version": "0.0.1",
  "description": "Todo list for introverts",
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/todoo/issues"
  },
  "homepage": "https://github.com/vesparny/todoo",
  "scripts": {
    "dev": "electron . --debug",
    "start": "electron .",
    "test": "npm run lint",
    "lint": "standard",
    "build": "cross-env NODE_ENV=production webpack -p",
    "pack-darwin": "npm run build && electron-packager . --asar --overwrite --platform=darwin --arch=x64 --icon=static/app-icon.icns --prune=true --out=out"
  },
  "dependencies": {
    "application-config-path": "0.1.0",
    "classnames": "2.2.5",
    "cuid": "1.3.8",
    "electron-prebuilt": "1.2.0",
    "fs-plus": "2.9.1",
    "lowdb": "0.13.1",
    "menubar": "4.1.2",
    "moment": "2.14.1",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "tachyons": "4.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.9.0",
    "cross-env": "1.0.8",
    "devtron": "1.2.1",
    "electron-debug": "1.0.1",
    "electron-packager": "7.2.0",
    "standard": "7.1.1",
    "webpack": "1.13.1"
  }
