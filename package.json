{
  "name": "todoo",
  "productName": "Todoo",
  "private": true,
  "version": "1.2.0",
  "description": "Todo app for introverts",
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/todoo/issues"
  },
  "homepage": "https://github.com/vesparny/todoo",
  "scripts": {
    "dev": "electron . --debug",
    "start": "electron .",
    "test": "npm run lint",
    "lint": "standard",
    "pack-all": "npm run build && npm run pack-mac && npm run pack-win && npm run pack-linux && node script/release.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "pack-mac": "electron-packager . --asar --overwrite --platform=darwin --arch=x64 --icon=static/app-icon.icns --prune=true --out=out --app-version=$npm_package_version",
    "pack-win": "electron-packager . Todoo --asar --overwrite --platform=win32 --arch=ia32 --icon=astatic/app-icon.ico --prune=true --out=out --version-string.CompanyName='Alessandro Arnodo' --version-string.FileDescription='Todo app for introverts' --version-string.ProductName='Todo app for introverts'",
    "pack-linux": "electron-packager . --asar --overwrite --platform=linux --arch=x64 --icon=astatic/app-icon.png--prune=true --out=out",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "application-config-path": "0.1.0",
    "autolinker": "0.27.0",
    "classnames": "2.2.5",
    "cuid": "1.3.8",
    "electron-is-dev": "0.1.1",
    "electron-prebuilt": "1.2.6",
    "fs-plus": "2.9.1",
    "lowdb": "0.13.1",
    "menubar": "5.0.0",
    "moment": "2.14.1",
    "react": "15.2.0",
    "react-addons-css-transition-group": "15.2.1",
    "react-addons-shallow-compare": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-virtualized": "7.12.1",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "request": "2.73.0",
    "reselect": "2.5.3",
    "semver": "5.2.0",
    "tachyons": "4.0.2",
    "tachyons-flex": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-register": "6.9.0",
    "conventional-changelog-cli": "1.2.0",
    "cross-env": "1.0.8",
    "devtron": "1.2.1",
    "electron-debug": "1.0.1",
    "electron-packager": "7.3.0",
    "json-loader": "0.5.4",
    "standard": "7.1.2",
    "webpack": "1.13.1"
  }
}
